{
  "version": 2,
  "name": "my-vercel-project",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "MY_SECRET_KEY": "@my-secret-key"
  },
  "github": {
    "silent": true
  },
  "domains": ["demo.perfectcorp.com"]
}
